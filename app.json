{
  "expo": {
    "name": "Fremont",
    "slug": "fremont-app",
    "owner": "mmortada685",
    "version": "0.53",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "fhs",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/adaptive-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#f8f8f8"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mmortada.fremontapp",
      "entitlements": {
        "aps-environment": "development"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/iconA.png",
        "backgroundColor": "#fff"
      },
      "package": "com.mmortada.fremontapp",
      "googleServicesFile": "./google-services.json"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/Inter-Bold.ttf"]
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "bbe3b165-5a10-4b8c-b739-d444596e60f6"
      }
    }
  }
}
